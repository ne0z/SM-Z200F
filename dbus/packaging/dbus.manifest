<manifest>
  <define>
    <domain name="dbus"/>
  </define>
  <assign>
    <filesystem path="/usr/bin/*" label="_" exec_label="none" />
    <filesystem path="/usr/bin/dbus-daemon" label="dbus" exec_label="dbus" />
    <filesystem path="/usr/lib/dbus-1/dbus-daemon-launch-helper" label="_" exec_label="none" />
    <filesystem path="/usr/lib/systemd/*" label="_" exec_label="none" />
  </assign>
  <request>
    <domain name="dbus"/>
  </request>
</manifest>
